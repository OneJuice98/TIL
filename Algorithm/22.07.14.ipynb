    "N = int(input())\n",
    "\n",
    "users = []\n",
    "for i in range(N):\n",
    "    age, name = map(str, input().split())\n",
    "    users.append([int(age), name, i])\n",
    "    \n",
    "users = sorted(users, key=lambda age : age[0])\n",
    "for i in users:\n",
    "    print(i[0], end=\" \")\n",
    "    print(i[1])\n"
   ]
  }
